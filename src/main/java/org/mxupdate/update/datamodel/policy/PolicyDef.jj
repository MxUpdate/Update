}

<DEFAULT> TOKEN : /** State and depending signature and trigger Tokens */
{
      <REGISTEREDNAME:                      "registeredName">                                       : S_EXPECTED
    | <REVISION:                            "revision">                                             : B_EXPECTED
    | <MINORREVISION:                       "minorrevision">                                        : B_EXPECTED
    | <MAJORREVISION:                       "majorrevision">                                        : B_EXPECTED
    | <VERSION:                             "version">                                              : B_EXPECTED
    | <PROMOTE:                             "promote">                                              : B_EXPECTED
    | <CHECKOUTHISTORY:                     "checkouthistory">                                      : B_EXPECTED
    | <ENFORCERESERVEACCESS:                "enforcereserveaccess">                                 : DEFAULT
    | <ENFORCERESERVEACCESS_TRUE:           "enforcereserveaccess" ([" ","\t","\n","\r"])+ (["t","T"] ["r","R"] ["u","U"] ["e","E"] | "\"" ["t","T"] ["r","R"] ["u","U"] ["e","E"] "\"")>
                                                                                                    : DEFAULT
    | <ENFORCERESERVEACCESS_NOT:            "notenforcereserveaccess">                              : DEFAULT
    | <ENFORCERESERVEACCESS_EXCLAMATION:    "!enforcereserveaccess">                                : DEFAULT
